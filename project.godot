; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="3D test"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
XRPlayerGlobals="*res://xr_level_startup/XRPlayerGlobals.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-xr-tools/plugin.cfg", "res://addons/godotopenxrvendors/plugin.cfg")

[filesystem]

import/fbx/enabled=false

[physics]

3d/physics_engine="JoltPhysics3D"
jolt_3d/collisions/areas_detect_static_bodies=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true

[shader_globals]

outline_width={
"type": "float",
"value": 1.0
}
outline_color={
"type": "color",
"value": Color(0, 0, 0, 1)
}
specular_smoothness={
"type": "float",
"value": 0.01
}
fresnel_smoothness={
"type": "float",
"value": 0.01
}
diffuse_curve={
"type": "sampler2D",
"value": ""
}

[xr]

openxr/enabled=true
shaders/enabled=true
